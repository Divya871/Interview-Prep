void binSort(int A[], int N)
{
    int i=0;
    int pt1=-1,pt2=N-1;
    while(i<=pt2)
    {
        if(A[i]==0)
        {
            pt1++;
            swap(A[i],A[pt1]);
            i++;
        }
        else if(A[i]==2)
        {
            pt2--;
            swap(A[i],A[pt2]);
        }
        else{
        i++;
     }
   }
 }
