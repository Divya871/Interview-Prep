WORSE COMPLEXITY->https://afteracademy.com/blog/check-for-balanced-parentheses-in-an-expression

TC=O(N),SC=O(N)
 bool isValid(string s) {
        int n=s.length();
        stack<int>st;
        for(int i=0;i<n;i++)
        {
          char ch=s[i];
          if(ch=='(' || ch=='{' || ch=='['){st.push(ch);}
          else  if(st.size()==0){return false;}
                 else if(ch==')' && st.top()!='('){return false;}
                 else if(ch=='}' && st.top()!='{'){return false;}
                 else if(ch==']' && st.top()!='['){return false;}
                 else {st.pop();}}
                 
        
  return st.size()==0;
    }
